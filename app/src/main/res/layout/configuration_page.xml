<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/app_background">

    <ScrollView
        android:id="@+id/scrollView"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:fillViewport="true"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/navbar_include"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_start"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.03" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_end"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.97" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_mleft"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.47" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_mright"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.53" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_middle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.5" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_top"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.08" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_bottom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.7" />

        <ImageView
            android:id="@+id/TopBar"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:src="@drawable/top_bar_1"
            android:scaleType="fitXY"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            tools:ignore="ContentDescription" />

        <ImageView
            android:id="@+id/SamsaraLogo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/samsara_logo_1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="ContentDescription" />


            <TextView
                android:id="@+id/tvConfig"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="50dp"
                android:layout_marginTop="20dp"
                android:text="@string/config"
                android:textColor="#C44F4F"
                android:textSize="34sp"
                app:layout_constraintStart_toEndOf="@id/SamsaraLogo"
                app:layout_constraintTop_toTopOf="@id/SamsaraLogo" />

            <TextView
            android:id="@+id/tvGeneral"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/padding_sm"
            android:layout_marginTop="@dimen/padding_md"
            android:text="@string/general"
            android:textColor="#C44F4F"
            android:textSize="24sp"
            app:layout_constraintStart_toStartOf="@id/guideline_start"
            app:layout_constraintTop_toBottomOf="@+id/SamsaraLogo" />

        <TextView
            android:id="@+id/tvAutoStart"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/padding_sm"
            android:layout_marginTop="@dimen/padding_md"
            android:text="@string/auto_start_on_boot"
            android:textColor="#FFFFFF"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="@id/guideline_start"
            app:layout_constraintTop_toBottomOf="@+id/tvGeneral" />

        <androidx.appcompat.widget.SwitchCompat
            android:id="@+id/switch1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/padding_md"
            android:layout_marginEnd="@dimen/padding_sm"
            android:scaleX="1.5"
            android:scaleY="1.5"
            app:layout_constraintBottom_toBottomOf="@+id/tvAutoStart"
            app:layout_constraintEnd_toEndOf="@id/guideline_end"
            app:layout_constraintTop_toBottomOf="@id/tvGeneral" />

        <TextView
            android:id="@+id/tvDashPass"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/padding_sm"
            android:layout_marginTop="@dimen/padding_md"
            android:backgroundTintMode="src_in"
            android:text="@string/dashboard_password"
            android:textColor="#FFFFFF"
            android:textSize="16sp"
            app:layout_constraintTop_toBottomOf="@+id/tvAutoStart"
            app:layout_constraintStart_toStartOf="@+id/guideline_start" />

        <EditText
            android:id="@+id/DashPassBox"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:scaleType="fitXY"
            android:layout_marginStart="@dimen/padding_md"
            android:layout_marginTop="@dimen/padding_sm"
            android:hint="@string/password"
            android:textColorHint="#a2a2a2"
            android:paddingStart="@dimen/text_sm"
            android:background="@drawable/configuration_3"
            app:layout_constraintTop_toBottomOf="@+id/tvDashPass"
            app:layout_constraintStart_toStartOf="@+id/guideline_start"
            app:layout_constraintEnd_toEndOf="@id/guideline_end"
            tools:ignore="Autofill,TextFields" />

        <TextView
            android:id="@+id/tvNetwork"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/padding_sm"
            android:layout_marginTop="@dimen/padding_md"
            android:text="@string/network"
            android:textColor="#C44F4F"
            android:textSize="24sp"
            app:layout_constraintStart_toStartOf="@+id/guideline_start"
            app:layout_constraintTop_toBottomOf="@+id/DashPassBox" />


        <TextView
            android:id="@+id/tvWebDashboardPort"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/padding_sm"
            android:layout_marginTop="@dimen/padding_md"
            android:text="@string/web_dashboard_port"
            android:textColor="#FFFFFF"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="@id/guideline_start"
            app:layout_constraintTop_toBottomOf="@+id/tvNetwork" />

        <EditText
            android:id="@+id/WebPortBox"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:scaleType="fitXY"
            android:layout_marginStart="@dimen/padding_md"
            android:layout_marginTop="@dimen/padding_sm"
            android:hint="@string/_8080"
            android:textColorHint="#a2a2a2"
            android:paddingStart="@dimen/text_sm"
            android:background="@drawable/configuration_3"
            app:layout_constraintTop_toBottomOf="@+id/tvWebDashboardPort"
            app:layout_constraintStart_toStartOf="@+id/guideline_start"
            app:layout_constraintEnd_toEndOf="@id/guideline_end"
            tools:ignore="Autofill,TextFields" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/padding_md"
            android:layout_marginStart="@dimen/padding_sm"
            android:text="@string/ssh_port"
            android:textColor="#FFFFFF"
            android:textSize="16sp"
            app:layout_constraintTop_toBottomOf="@+id/WebPortBox"
            app:layout_constraintStart_toStartOf="@+id/guideline_start" />


        <EditText
            android:id="@+id/SSHPortBox"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/padding_md"
            android:layout_marginTop="@dimen/padding_sm"
            android:scaleType="fitXY"
            android:hint="@string/_2222"
            android:textColorHint="#a2a2a2"
            android:paddingStart="15dp"
            android:paddingEnd="10dp"
            android:background="@drawable/configuration_3"
            app:layout_constraintTop_toBottomOf="@+id/textView11"
            app:layout_constraintStart_toStartOf="@+id/guideline_start"
            app:layout_constraintEnd_toEndOf="@id/guideline_end"
            tools:ignore="Autofill,TextFields" />


        <TextView
            android:id="@+id/tvLinuxEnvironemnt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/padding_sm"
            android:layout_marginTop="@dimen/padding_md"
            android:text="@string/linux_environment"
            android:textColor="#C44F4F"
            android:textSize="24sp"
            app:layout_constraintStart_toStartOf="@+id/guideline_start"
            app:layout_constraintTop_toBottomOf="@+id/SSHPortBox" />

        <TextView
            android:id="@+id/tvMonitoring"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/padding_sm"
            android:layout_marginTop="@dimen/padding_md"
            android:text="@string/monitoring"
            android:textColor="#C44F4F"
            android:textSize="24sp"
            app:layout_constraintStart_toStartOf="@+id/guideline_start"
            app:layout_constraintTop_toBottomOf="@+id/tvLinuxEnvironemnt" />

        <TextView
            android:id="@+id/tvMonitoringInterval"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/padding_md"
            android:layout_marginTop="@dimen/padding_lg"
            android:text="@string/monitoring_interval"
            android:textColor="#FFFFFF"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="@+id/guideline_start"
            app:layout_constraintTop_toBottomOf="@+id/tvMonitoring" />

        <Spinner
            android:id="@+id/spinnerMonitoringInterval"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/configuration_4"
            android:padding="10dp"
            app:layout_constraintTop_toTopOf="@+id/tvMonitoringInterval"
            app:layout_constraintBottom_toBottomOf="@+id/tvMonitoringInterval"
            app:layout_constraintEnd_toEndOf="@id/guideline_end" />


        <TextView
            android:id="@+id/tvTempAlert"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/padding_md"
            android:layout_marginTop="@dimen/padding_lg"
            android:text="@string/temperature_alert"
            android:textColor="#FFFFFF"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="@id/guideline_start"
            app:layout_constraintTop_toBottomOf="@+id/tvMonitoringInterval" />

        <EditText
            android:id="@+id/TempBox"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/padding_md"
            android:hint="@string/_60c"
            android:textSize="20sp"
            android:textColorHint="#a2a2a2"
            android:paddingStart="20dp"
            android:paddingBottom="10dp"
            android:background="@drawable/configuration_6"
            app:layout_constraintEnd_toEndOf="@+id/spinnerMonitoringInterval"
            app:layout_constraintTop_toBottomOf="@+id/spinnerMonitoringInterval"
            app:layout_constraintStart_toStartOf="@+id/spinnerMonitoringInterval"
            tools:ignore="Autofill,TextFields" />

        <TextView
            android:id="@+id/tvCommunityProjects"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/padding_sm"
            android:layout_marginTop="@dimen/padding_md"
            android:text="@string/community_projects"
            android:textColor="#C44F4F"
            android:textSize="24sp"
            app:layout_constraintStart_toStartOf="@id/guideline_start"
            app:layout_constraintTop_toBottomOf="@+id/tvTempAlert" />

        <TextView
            android:id="@+id/tvAutoUpdate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/padding_md"
            android:layout_marginTop="@dimen/padding_lg"
            android:text="@string/auto_update_project"
            android:textColor="#FFFFFF"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="@id/guideline_start"
            app:layout_constraintTop_toBottomOf="@+id/tvCommunityProjects" />

        <androidx.appcompat.widget.SwitchCompat
            android:id="@+id/switch2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/padding_md"
            android:layout_marginEnd="@dimen/padding_sm"
            android:scaleX="1.5"
            android:scaleY="1.5"
            app:layout_constraintBottom_toBottomOf="@+id/tvAutoUpdate"
            app:layout_constraintEnd_toEndOf="@id/guideline_end"
            app:layout_constraintTop_toTopOf="@+id/tvAutoUpdate" />

        <TextView
            android:id="@+id/tvInstallDir"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/padding_md"
            android:layout_marginTop="@dimen/padding_lg"
            android:text="@string/installation_directory"
            android:textColor="#FFFFFF"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="@id/guideline_start"
            app:layout_constraintTop_toBottomOf="@+id/tvAutoUpdate" />

        <EditText
            android:id="@+id/InstallDir2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:scaleType="fitXY"
            android:layout_marginTop="@dimen/padding_md"
            android:layout_marginStart="@dimen/padding_sm"
            android:layout_marginEnd="@dimen/padding_md"
            android:hint="@string/data"
            android:textColorHint="#a2a2a2"
            android:paddingStart="10dp"
            android:background="@drawable/configuration_7"
            app:layout_constraintStart_toStartOf="@+id/guideline_start"
            app:layout_constraintEnd_toStartOf="@id/InstallDir3"
            app:layout_constraintTop_toBottomOf="@+id/tvInstallDir"
            tools:ignore="Autofill,TextFields" />

        <ImageButton
            android:id="@+id/InstallDir3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="#00FFFFFF"
            android:src="@drawable/configuration_8"
            app:layout_constraintBottom_toBottomOf="@id/InstallDir2"
            app:layout_constraintEnd_toEndOf="@id/guideline_end"
            app:layout_constraintTop_toTopOf="@+id/InstallDir2"
            tools:ignore="ContentDescription" />

        <TextView
            android:id="@+id/tvAdvanced"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/padding_sm"
            android:layout_marginTop="@dimen/padding_md"
            android:text="@string/advanced"
            android:textColor="#C44F4F"
            android:textSize="24sp"
            app:layout_constraintStart_toStartOf="@id/guideline_start"
            app:layout_constraintTop_toBottomOf="@+id/InstallDir2" />


        <TextView
            android:id="@+id/tvLowStorageWarning"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/padding_md"
            android:layout_marginTop="@dimen/padding_lg"
            android:text="@string/low_storage_warning"
            android:textColor="#FFFFFF"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="@id/guideline_start"
            app:layout_constraintTop_toBottomOf="@+id/tvAdvanced" />

        <EditText
            android:id="@+id/LowStorageBox"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/padding_md"
            android:layout_marginEnd="@dimen/padding_sm"
            android:hint="@string/_8gb"
            android:textSize="15sp"
            android:textColorHint="#a2a2a2"
            android:paddingStart="20dp"
            android:paddingBottom="10dp"
            android:background="@drawable/configuration_6"
            app:layout_constraintEnd_toEndOf="@id/guideline_end"
            app:layout_constraintBottom_toBottomOf="@+id/tvLowStorageWarning"
            app:layout_constraintTop_toTopOf="@+id/tvLowStorageWarning"
            tools:ignore="Autofill,TextFields" />


        <ImageButton
            android:id="@+id/ExportBtn"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/padding_lg"
            android:scaleType="fitXY"
            android:backgroundTint="#00FFFFFF"
            android:src="@drawable/configuration_9"
            app:layout_constraintStart_toStartOf="@id/guideline_start"
            app:layout_constraintEnd_toEndOf="@id/guideline_middle"
            app:layout_constraintTop_toBottomOf="@+id/tvLowStorageWarning"
            tools:ignore="ContentDescription" />


        <TextView
            android:id="@+id/tvExport"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="@string/export_configuration"
            android:gravity="center"
            android:textColor="#FFFFFF"
            app:layout_constraintEnd_toEndOf="@+id/ExportBtn"
            app:layout_constraintTop_toTopOf="@+id/ExportBtn"
            app:layout_constraintStart_toStartOf="@+id/ExportBtn"
            app:layout_constraintBottom_toBottomOf="@+id/ExportBtn"/>

        <ImageButton
            android:id="@+id/ImportBtn"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:backgroundTint="#00FFFFFF"
            android:scaleType="fitXY"
            android:src="@drawable/configuration_9"
            app:layout_constraintEnd_toEndOf="@+id/guideline_end"
            app:layout_constraintStart_toStartOf="@id/guideline_middle"
            app:layout_constraintBottom_toBottomOf="@+id/ExportBtn"
            app:layout_constraintTop_toTopOf="@+id/ExportBtn"
            tools:ignore="ContentDescription" />

        <TextView
            android:id="@+id/tvImport"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/padding_sm"
            android:layout_marginEnd="@dimen/padding_sm"
            android:text="@string/import_configuration"
            android:textColor="#FFFFFF"
            app:layout_constraintEnd_toEndOf="@+id/ImportBtn"
            app:layout_constraintTop_toTopOf="@+id/ImportBtn"
            app:layout_constraintStart_toStartOf="@+id/ImportBtn"
            app:layout_constraintBottom_toBottomOf="@+id/ImportBtn" />

        <ImageButton
            android:id="@+id/ResetBtn"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/padding_sm"
            android:backgroundTint="#00FFFFFF"
            android:scaleType="fitXY"
            android:src="@drawable/configuration_9"
            app:layout_constraintEnd_toEndOf="@+id/ExportBtn"
            app:layout_constraintStart_toStartOf="@id/ExportBtn"
            app:layout_constraintTop_toBottomOf="@+id/ExportBtn"
            tools:ignore="ContentDescription" />


        <TextView
            android:id="@+id/tvReset"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/reset_to_defaults"
            android:textColor="#FFFFFF"
            app:layout_constraintEnd_toEndOf="@+id/ResetBtn"
            app:layout_constraintTop_toTopOf="@+id/ResetBtn"
            app:layout_constraintStart_toStartOf="@+id/ResetBtn"
            app:layout_constraintBottom_toBottomOf="@+id/ResetBtn" />

        <ImageButton
            android:id="@+id/SaveBtn"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/padding_sm"
            android:scaleType="fitXY"
            android:backgroundTint="#00FFFFFF"
            android:src="@drawable/configuration_9"
            app:layout_constraintEnd_toEndOf="@+id/ImportBtn"
            app:layout_constraintStart_toStartOf="@id/ImportBtn"
            app:layout_constraintTop_toBottomOf="@+id/ImportBtn"
            tools:ignore="ContentDescription" />

        <TextView
            android:id="@+id/tvSave"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="6dp"
            android:text="@string/save_changes"
            android:textColor="#FFFFFF"
            app:layout_constraintEnd_toEndOf="@+id/SaveBtn"
            app:layout_constraintStart_toStartOf="@+id/SaveBtn"
            app:layout_constraintTop_toTopOf="@+id/SaveBtn"
            app:layout_constraintBottom_toBottomOf="@+id/SaveBtn"/>

        <TextView
            android:id="@+id/tvAbout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/padding_sm"
            android:layout_marginTop="@dimen/padding_md"
            android:text="@string/about"
            android:textColor="#C44F4F"
            android:textSize="24sp"
            app:layout_constraintStart_toStartOf="@id/guideline_start"
            app:layout_constraintTop_toBottomOf="@+id/ResetBtn" />

        <TextView
            android:id="@+id/tvAppVersion"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/padding_md"
            android:layout_marginTop="@dimen/padding_lg"
            android:text="@string/app_version"
            android:textColor="#FFFFFF"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="@id/guideline_start"
            app:layout_constraintTop_toBottomOf="@+id/tvAbout" />

        <TextView
            android:id="@+id/tvAppVersion2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/padding_md"
            android:text="@string/textview"
            android:textColor="#FFFFFF"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="@+id/tvAppVersion"
            app:layout_constraintStart_toEndOf="@+id/tvAppVersion"
            app:layout_constraintTop_toTopOf="@+id/tvAppVersion" />

        <TextView
            android:id="@+id/tvBusyBox"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/padding_md"
            android:layout_marginTop="@dimen/padding_lg"
            android:text="@string/busybox"
            android:textColor="#FFFFFF"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="@id/guideline_start"
            app:layout_constraintTop_toBottomOf="@+id/tvAppVersion" />

        <TextView
            android:id="@+id/tvBusyBox2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/textview"
            android:textColor="#FFFFFF"
            android:textSize="16sp"
            android:layout_marginStart="@dimen/padding_md"
            app:layout_constraintBottom_toBottomOf="@+id/tvBusyBox"
            app:layout_constraintStart_toEndOf="@+id/tvBusyBox"
            app:layout_constraintTop_toTopOf="@+id/tvBusyBox"  />

        <ImageButton
            android:id="@+id/imageView14"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/padding_lg"
            android:scaleType="fitXY"
            android:backgroundTint="#00FFFFFF"
            app:layout_constraintEnd_toEndOf="@id/guideline_end"
            app:layout_constraintStart_toStartOf="@id/guideline_start"
            app:layout_constraintTop_toBottomOf="@+id/tvBusyBox"
            android:src="@drawable/configuration_10"
            tools:ignore="ContentDescription" />

        <TextView
            android:id="@+id/tvSystemFiles"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/press_to_view_system_files"
            android:textColor="#FFFFFF"
            app:layout_constraintEnd_toEndOf="@+id/imageView14"
            app:layout_constraintStart_toStartOf="@+id/imageView14"
            app:layout_constraintTop_toTopOf="@+id/imageView14"
            app:layout_constraintBottom_toBottomOf="@+id/imageView14"/>

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/browse"
                app:layout_constraintBottom_toBottomOf="@+id/InstallDir3"
                app:layout_constraintEnd_toEndOf="@+id/InstallDir3"
                app:layout_constraintStart_toStartOf="@+id/InstallDir3"
                app:layout_constraintTop_toTopOf="@+id/InstallDir3" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

    <include
        android:id="@+id/navbar_include"
        layout="@layout/navbar_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
